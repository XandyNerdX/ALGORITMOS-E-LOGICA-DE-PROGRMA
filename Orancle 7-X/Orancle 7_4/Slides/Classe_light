package orancle7.pkg4;


public class UIWheelLight {
    // Campos
    public UIWheel wheel;
    public double rotation;
    public boolean isLit;

    // Construtor
    public UIWheelLight(UIWheel w, double r, boolean l) {
        wheel = w;
        rotation = r;
        isLit = l;

  

    } // fim Construtor                                     


}// fim da classe UIWheelLigh