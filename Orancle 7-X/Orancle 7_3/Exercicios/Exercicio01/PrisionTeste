
public class Display {
    public static void main(String[] args) {
        Prisioner p01 = new Prisioner();
        Prisioner p02 = new Prisioner();

        //Primeira ocorrência
        p01.name = "Bubba";
        p01.altura = 2.08;
        p01.sentence = 4;
        //Segunda repetição
        p02.name = "Twitch";
        p02.altura = 1.73;
        p02.sentence = 3;        


    }//fim do método main

}//fim da classe prisonteste
